@model ASC.Common.VehicleFormModel
<div class="form-group row" style="padding-right: 0.75rem;padding-left: 0.75rem;">


    @Html.LabelFor(model => model.vehicle.ModelId, htmlAttributes: new { @class = "control-label col-md-3 m-t-15" })
    <div class="col-md-8">
        @Html.DropDownListFor(model => model.vehicle.ModelId, new SelectList(Model.models, "Id", "Name"), "Select", new { id = "myDropDownlistID2", @class = "form-control", required = "required" })
        @Html.ValidationMessageFor(model => model.vehicle.ModelId, "", new { @class = "text-danger" })
    </div>
</div>